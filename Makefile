CHEETAH = cheetah

################################################################
# Dependencies
################################################################

# Files
htmls = $(addsuffix .html, index people visitors mug)


# Targets
all: $(htmls)


################################################################
# Implicit rules
################################################################

# Production rules
%.html: %.tmpl layout.tmpl
	$(CHEETAH) fill $<
